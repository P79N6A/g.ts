{
  "name": "@gradii/g-workspace",
  "version": "0.0.0-alpha.0",
  "description": "A canvas library which providing 2d draw for G2.",
  "keywords": [
    "canvas",
    "G2",
    "2d draw",
    "typescript"
  ],
  "private": true,
  "devDependencies": {
    "@types/gulp": "^4.0.5",
    "@types/jasmine": "^3.3.0",
    "@types/node": "^10",
    "@types/shelljs": "^0.8.0",
    "@types/webpack-env": "^1.13.6",
    "awesome-typescript-loader": "^5.2.1",
    "chai": "~4.2.0",
    "event-simulate": "~1.0.0",
    "gulp": "^4",
    "gulp-rename": "^1.4.0",
    "gulpfile": "^1.0.2",
    "istanbul": "~0.4.5",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jasmine": "^3.3.0",
    "jquery": "~3.3.1",
    "karma": "^3.1.1",
    "karma-browserstack-launcher": "^1.3.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-remap-coverage": "^0.1.5",
    "karma-sauce-launcher": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.5",
    "reflect-metadata": "^0.1.12",
    "shelljs": "^0.8.3",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^5.3.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.1.2",
    "webpack-merge": "^4.1.4"
  },
  "scripts": {
    "build": "webpack",
    "build-lib": "babel src --out-dir lib",
    "test": "karma start karma.conf.js --single-run --no-auto-watch",
    "compress": "uglifyjs --compress --mangle --output dist/g-$npm_package_version.min.js -- build/g.js",
    "coverage": "npm run coverage-generator && npm run coverage-viewer",
    "coverage-viewer": "torch-coverage",
    "coverage-generator": "torch --coverage --compile --renderer --recursive test/unit",
    "dev": "webpack --config webpack-dev.config.js",
    "dist": "rm -rf dist && mkdir dist && npm run build && npm run compress",
    "prepublishOnly": "npm run build-lib && npm run dist",
    "tslint": "tslint packages",
    "tslint-fix": "tslint --fix packages/**/*.ts",
    "test:vector-match": "karma start libs/vector-math/karma.conf.js"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "codecov": "^3.1.0",
    "global": "^4.3.2",
    "tsconfig-paths": "^3.7.0"
  }
}
